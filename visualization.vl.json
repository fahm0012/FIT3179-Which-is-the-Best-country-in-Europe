{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Median Income in Different Countires of Europe in 2016",
  "width": 450,
  "height": 450,
  "data": {
    "url": "https://raw.githubusercontent.com/fahm0012/FIT3179-Which-is-the-Best-country-in-Europe/main/Europe.geo%20(1).json",
    "format": {"type": "topojson", "feature": "Europe"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/fahm0012/FIT3179-Which-is-the-Best-country-in-Europe/main/EuropeClean2016DataSet.csv"
        },
        "key": "country",
        "fields": ["MedianIncome", "Population"]
      }
    },
    {"calculate": "datum.MedianIncome", "as": "Median Income"}
  ],
  "projection": {"type": "mercator"},
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Median Income",
      "type": "quantitative",
      "scale": {
        "type": "threshold",
        "domain": [5000, 10000, 15000, 20000, 25000],
        "range": [
          "#ffffcc",
          "#c7e9b4",
          "#7fcdbb",
          "#41b6c4",
          "#2c7fb8",
          "#253494"
        ]
      }
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "Country"},
      {"field": "Median Income", "type": "quantitative"}
    ]
  },
  "config": {}
}
